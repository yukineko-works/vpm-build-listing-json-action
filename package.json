{
  "name": "vpm-build-listing-json-action",
  "description": "VPM Listing json builder",
  "version": "0.1.0",
  "author": "yukineko",
  "private": true,
  "homepage": "https://github.com/yukineko-works/vpm-build-listing-json-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukineko-works/vpm-build-listing-json-action.git"
  },
  "bugs": {
    "url": "https://github.com/yukineko-works/vpm-build-listing-json-action/issues"
  },
  "keywords": [],
  "exports": {
    ".": "./dist/index.js"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=21"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "all": "npm run format:write && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "node-fetch": "^2.6.6"
  },
  "devDependencies": {
    "@types/node": "^22.7.7",
    "@types/node-fetch": "2.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vercel/ncc": "^0.38.2",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.6.3"
  }
}
